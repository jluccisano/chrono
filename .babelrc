{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          },
          "react"
        ]
      ]
    }
  }
}
